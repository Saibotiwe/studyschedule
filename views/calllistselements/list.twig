{% extends '/views/layouts/main.twig' %}

{% block title %}{{ call_list.name }}{% endblock %}

{% block content %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <h1>{{ call_list.name }}</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                {% include '/views/particle/notice.twig' %}
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <a href="{{ this.createUrl('create', {list_id: App.request.param('list_id')}) }}"
                   class="btn btn-default pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                {% include '/views/calllistselements/_list.twig' %}
            </div>
        </div>
    </div>
{% endblock %}